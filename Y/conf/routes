# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                                       Application.index

GET 	/testdubbo									TestAction.dubboTest
GET 	/testscheduling								TestAction.getRouteListByOrgcode
GET 	/testsd 							TestAction.testsd
GET 	/getroute									TestAction.getRoute
GET 	/pullMileInfo									TestAction.pullMileInfo


GET 	/imgs								LoginAction.showimgs
# Ignore favicon requests
GET     /favicon.ico                            404

# Map static resources from the /app/public folder to the /public path
GET     /public/                                staticDir:public

# Catch all
*       /{controller}/{action}                  {controller}.{action}
